<h1><%= @task.name %></h1>

<div>
  <span>詳細：</span><%= @task.description %>
</div>

<div>
  <span>終了期限：</span><%= @task.expired_at %>
</div>

<div>
  <span>ラベル一覧：</span>
  <% @task.labels.each do | label | %>
    <%= label.name %>
  <% end %>
</div>

<div>
  <%= link_to '一覧に戻る', tasks_path %>
</div>
